"\n"
"post-install-EXAMPLES-on:\n"
"      ${MKDIR} ${STAGEDIR}${EXAMPLESDIR}\n"
"      cd ${WRKSRC}/ex &amp;&amp; ${COPYTREE_SHARE} . ${STAGEDIR}${EXAMPLESDIR}"
msgstr ""
"OPTIONS_DEFINE=\tDOCS EXAMPLES FOO BAR\n"
"OPTIONS_DEFAULT=\tFOO\n"
"OPTIONS_RADIO=\tSSL\n"
